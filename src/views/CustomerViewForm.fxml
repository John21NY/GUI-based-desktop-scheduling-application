        </TableView>
          <Button fx:id="backButton" layoutX="706.0" layoutY="554.0" mnemonicParsing="false" onAction="#pressBackButton" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: #FFFFFF#FFFFFF;" text="Back">
         <font>
            <Font name="Verdana" size="14.0" />
         </font>
      </Button>
          <Button fx:id="deleteButton" layoutX="616.0" layoutY="554.0" mnemonicParsing="false" onAction="#deleteButtonOnAction" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: #FFFFFF#FFFFFF;" text="Delete">
