      </Button>
          <Button fx:id="editButton" layoutX="526.0" layoutY="554.0" mnemonicParsing="false" onAction="#editButtonOnAction" prefHeight="25.0" prefWidth="80.0" style="-fx-background-color: #FFFFFF#FFFFFF;" text="Edit">
         <font>
            <Font name="Verdana" size="14.0" />
         </font>
      </Button>
          <Button fx:id="addButton" layoutX="436.0" layoutY="554.0" mnemonicParsing="false" onAction="#addButtonOnAction" prefHeight="27.0" prefWidth="80.0" style="-fx-background-color: #FFFFFF#FFFFFF;" text="Add">
              <padding>
                  <Insets right="10.0" />
              </padding>
         <font>
            <Font name="Verdana" size="14.0" />
         </font>
